    "multiply": {
      "description": "乘法运算",
      "args": [
        {
          "type": "jsonlang.Number",
          "name": "a",
          "required": true,
          "description": "第一个数"
        },
        {
          "type": "jsonlang.Number",
          "name": "b",
          "required": true,
          "description": "第二个数"
        }
      ],
      "return": {
        "type": "jsonlang.Number",
        "description": "两数之积"
      }
