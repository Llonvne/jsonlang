    "divide": {
      "description": "除法运算",
      "args": [
        {
          "type": "jsonlang.Number",
          "name": "a",
          "required": true,
          "description": "被除数"
        },
        {
          "type": "jsonlang.Number",
          "name": "b",
          "required": true,
          "description": "除数"
        }
      ],
      "return": {
        "type": "jsonlang.Number",
        "description": "两数之商"
      }
