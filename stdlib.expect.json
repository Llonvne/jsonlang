      "description": "获取数组元素",
      "args": [
        {
          "type": "jsonlang.Array",
          "name": "array",
          "required": true,
          "description": "目标数组"
        },
        {
          "type": "jsonlang.Number",
          "name": "index",
          "required": true,
          "description": "元素索引"
        }
      ],
      "return": {
        "type": "jsonlang.Any",
        "description": "数组元素"
      }
    },
    "array_set": {
      "description": "设置数组元素",
      "args": [
        {
          "type": "jsonlang.Array",
          "name": "array",
          "required": true,
          "description": "目标数组"
        },
        {
          "type": "jsonlang.Number",
          "name": "index",
          "required": true,
          "description": "元素索引"
        },
        {
          "type": "jsonlang.Any",
          "name": "value",
          "required": true,
          "description": "要设置的值"
        }
      ],
      "return": {
        "type": "jsonlang.Array",
        "description": "设置元素后的数组"
      }
    },
    "array_length": {
      "description": "获取数组长度",
      "args": [
        {
          "type": "jsonlang.Array",
          "name": "array",
          "required": true,
          "description": "目标数组"
        }
      ],
      "return": {
        "type": "jsonlang.Number",
        "description": "数组长度"
      }
    },
    "array_sort": {
      "description": "排序数组",
      "args": [
        {
          "type": "jsonlang.Array",
          "name": "array",
          "required": true,
          "description": "要排序的数组"
        },
        {
          "type": "jsonlang.Boolean",
          "name": "reverse",
          "required": false,
          "description": "是否逆序",
          "default": false
        }
      ],
      "return": {
        "type": "jsonlang.Array",
        "description": "排序后的数组"
      }
    },
    "array_reverse": {
      "description": "反转数组",
      "args": [
        {
          "type": "jsonlang.Array",
          "name": "array",
          "required": true,
          "description": "要反转的数组"
        }
      ],
      "return": {
        "type": "jsonlang.Array",
        "description": "反转后的数组"
      }
