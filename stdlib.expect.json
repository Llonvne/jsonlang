  "types": {
    "Unit": {
      "type": "unit",
      "description": "无返回值类型"
    },
    "String": {
      "type": "string",
      "description": "字符串类型"
    },
    "Number": {
      "type": "number",
      "description": "数字类型"
    },
    "Boolean": {
      "type": "boolean",
      "description": "布尔类型"
    },
    "Array": {
      "type": "array",
      "description": "数组类型"
    },
    "Any": {
      "type": "any",
      "description": "任意类型"
    }
