      "description": "获取当前时间戳",
      "args": [],
      "return": {
        "type": "jsonlang.Number",
        "description": "当前时间戳"
      }
    },
    "exit": {
      "description": "退出程序",
      "args": [
        {
          "type": "jsonlang.Number",
          "name": "code",
          "required": false,
