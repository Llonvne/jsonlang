    "subtract": {
      "description": "减法运算",
      "args": [
        {
          "type": "jsonlang.Number",
          "name": "a",
          "required": true,
          "description": "被减数"
        },
        {
          "type": "jsonlang.Number",
          "name": "b",
          "required": true,
          "description": "减数"
        }
      ],
      "return": {
        "type": "jsonlang.Number",
        "description": "两数之差"
      }
