    "write_file": {
      "description": "写入文件内容",
      "args": [
        {
          "type": "jsonlang.String",
          "name": "filename",
          "required": true,
          "description": "文件名"
        },
        {
          "type": "jsonlang.String",
          "name": "content",
          "required": true,
          "description": "要写入的内容"
        }
      ],
      "return": {
        "type": "jsonlang.Boolean",
        "description": "是否成功"
      }
    },
    "add": {
      "description": "加法运算",
      "args": [
        {
          "type": "jsonlang.Number",
          "name": "a",
          "required": true,
          "description": "第一个数"
        },
        {
          "type": "jsonlang.Number",
          "name": "b",
          "required": true,
          "description": "第二个数"
        }
      ],
      "return": {
        "type": "jsonlang.Number",
        "description": "两数之和"
      }
