      "description": "平方根",
      "args": [
        {
          "type": "jsonlang.Number",
          "name": "x",
          "required": true,
          "description": "要计算平方根的数"
        }
      ],
      "return": {
        "type": "jsonlang.Number",
        "description": "平方根"
      }
    },
    "abs": {
      "description": "绝对值",
      "args": [
        {
          "type": "jsonlang.Number",
          "name": "x",
          "required": true,
          "description": "要计算绝对值的数"
        }
      ],
      "return": {
        "type": "jsonlang.Number",
        "description": "绝对值"
      }
    },
    "floor": {
      "description": "向下取整",
      "args": [
        {
          "type": "jsonlang.Number",
          "name": "x",
          "required": true,
          "description": "要取整的数"
        }
      ],
      "return": {
        "type": "jsonlang.Number",
        "description": "向下取整结果"
      }
    },
    "ceil": {
      "description": "向上取整",
      "args": [
        {
          "type": "jsonlang.Number",
          "name": "x",
          "required": true,
          "description": "要取整的数"
        }
      ],
      "return": {
        "type": "jsonlang.Number",
        "description": "向上取整结果"
      }
