      "args": [
        {
          "type": "jsonlang.Any",
          "name": "value",
          "required": true,
          "description": "要检查的值"
        }
      ],
      "return": {
        "type": "jsonlang.Boolean",
        "description": "是否为布尔值"
      }
    }
