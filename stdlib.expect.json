{
  "metadata": {
    "package": "jsonlang",
    "version": "1.0.0",
    "description": "JSONLang标准库接口规范"
  },
  "interface": {
    "println": {
      "description": "打印并换行",
      "args": [
        {
          "type": "jsonlang.String",
          "name": "message",
          "required": true,
          "description": "要打印的消息"
        }
      ],
      "return": {
        "type": "jsonlang.Unit",
        "description": "无返回值"
      }
    },
    "print": {
      "description": "打印不换行",
      "args": [
        {
          "type": "jsonlang.String",
          "name": "message",
          "required": true,
          "description": "要打印的消息"
        }
      ],
      "return": {
        "type": "jsonlang.Unit",
        "description": "无返回值"
      }
    },
    "input": {
      "description": "获取用户输入",
      "args": [
        {
          "type": "jsonlang.String",
          "name": "prompt",
          "required": false,
          "description": "提示信息",
          "default": ""
        }
      ],
      "return": {
        "type": "jsonlang.String",
        "description": "用户输入的内容"
      }
    },
    "read_file": {
      "description": "读取文件内容",
      "args": [
        {
          "type": "jsonlang.String",
          "name": "filename",
          "required": true,
          "description": "文件名"
        }
      ],
      "return": {
        "type": "jsonlang.String",
        "description": "文件内容"
      }
